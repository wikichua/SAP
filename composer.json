{
    "name": "wikichua/sap",
    "description": "Simple Admin Panel. Similar to Simple Control Panel with little tweaking only",
    "license": "MIT",
    "authors": [{
        "name": "Wiki Chua",
        "email": "wikichua@gmail.com",
        "homepage": "https://vam.latteichi.net"
    }],
    "homepage": "https://github.com/wikichua/sap",
    "keywords": [
        "Laravel",
        "sap",
        "Admin Panel",
        "Control Panel",
        "Admin",
        "CMS"
    ],
    "suggest": {
        "laravel/ui": "Yes! We do need this package :-)"
    },
    "require": {
        "illuminate/support": "~5|~6|~7",
        "laravel/helpers": "^1.2",
        "tightenco/ziggy": "^0.9.4",
        "barryvdh/laravel-debugbar": "^3.3",
        "laravel/sanctum": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Wikichua\\SAP\\": "src/"
        },
        "files": [
            "src/Http/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Wikichua\\SAP\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wikichua\\SAP\\SAPServiceProvider"
            ],
            "aliases": {
                "SAP": "Wikichua\\SAP\\Facades\\SAP"
            }
        }
    }
}
