{
    "name": "wikichua/sap",
    "description": "Simple Admin Panel. Similar to Simple Control Panel with little tweaking only",
    "license": "MIT",
    "authors": [{
        "name": "Wiki Chua",
        "email": "wikichua@gmail.com",
        "homepage": "https://vam.latteichi.net"
    }],
    "homepage": "https://github.com/wikichua/sap",
    "keywords": [
        "Laravel",
        "sap",
        "Admin Panel",
        "Control Panel",
        "Admin",
        "CMS"
    ],
    "suggest": {
        "laravel/ui": "Yes! We do need this package :-)",
        "laravel/horizon": "Yes! We do need this package :-)"
    },
    "require": {
        "illuminate/support": "~5|~6|~7|~8",
        "laravel/helpers": "^1.3",
        "tightenco/ziggy": "^0.9.4",
        "barryvdh/laravel-debugbar": "^3.5",
        "laravel/sanctum": "^2.6",
        "lab404/laravel-impersonate": "^1.7",
        "rap2hpoutre/laravel-log-viewer": "^1.7",
        "pusher/pusher-php-server": "^4.1",
        "laravel/socialite": "^5.0",
        "unisharp/laravel-filemanager": "^2.1",
        "predis/predis": "^1.1",
        "laravel/scout": "^8.3",
        "matchish/laravel-scout-elasticsearch": "^4.0",
        "elasticsearch/elasticsearch": "^7.9",
        "lionix/seo-manager": "^1.3",
        "rap2hpoutre/fast-excel": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Wikichua\\SAP\\": "src/"
        },
        "files": [
            "src/Http/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Wikichua\\SAP\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wikichua\\SAP\\SAPServiceProvider"
            ],
            "aliases": {
                "SAP": "Wikichua\\SAP\\Facades\\SAP"
            }
        }
    }
}
