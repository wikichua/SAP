{
    "name": "wikichua/sap",
    "description": "Simple Admin Panel. Similar to Simple Control Panel with little tweaking only",
    "license": "MIT",
    "authors": [{
        "name": "Wiki Chua",
        "email": "wikichua@gmail.com",
        "homepage": "https://vam.latteichi.net"
    }],
    "homepage": "https://github.com/wikichua/sap",
    "keywords": [
        "Laravel",
        "sap",
        "Admin Panel",
        "Control Panel",
        "Admin",
        "CMS"
    ],
    "suggest": {
        "laravel/ui": "^2.4|^3.0",
        "laravel/horizon": "Yes! We do need this package :-)"
    },
    "require": {
        "pusher/pusher-php-server": "^4.1",
        "predis/predis": "^1.1",
        "laravel/helpers": "dev-master",
        "laravel/sanctum": "dev-master",
        "laravel/socialite": "dev-master",
        "lab404/laravel-impersonate": "dev-master",
        "rap2hpoutre/laravel-log-viewer": "^1.7",
        "rap2hpoutre/fast-excel": "^2.1",
        "tightenco/ziggy": "^0.9.4",
        "barryvdh/laravel-debugbar": "^3.5",
        "paragonie/sodium_compat": "^1.13",
        "psr/cache": "dev-master",
        "unisharp/laravel-filemanager": "dev-master",
        "lionix/seo-manager": "dev-master",
        "intervention/imagecache": "^2.5",
        "realrashid/sweet-alert": "dev-master",
        "arcanedev/laravel-markdown": "dev-master",
        "spatie/async": "dev-master",
        "doctrine/cache": "^1.10",
        "jenssegers/agent": "dev-master",
        "spatie/laravel-honeypot": "dev-master",
        "infureal/artisan-gui": "dev-main",
        "league/flysystem": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Wikichua\\SAP\\": "src/"
        },
        "files": [
            "src/Http/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Wikichua\\SAP\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wikichua\\SAP\\SAPServiceProvider"
            ],
            "aliases": {
                "SAP": "Wikichua\\SAP\\Facades\\SAP"
            }
        }
    }
}
